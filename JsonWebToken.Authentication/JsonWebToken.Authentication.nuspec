<?xml version="1.0" encoding="utf-8"?>
<package>
<metadata>
  <!-- The identifier that must be unique within the hosting gallery -->
  <id>$id$</id>

  <!-- The package version number that is used when resolving dependencies -->
  <version>$version$</version>

  <!-- Authors contain text that appears directly on the gallery -->
  <authors>$author$</authors>

  <!-- Project URL provides a link for the gallery -->
  <projectUrl>$projectUrl$</projectUrl>

  <!-- License information is displayed on the gallery -->
  <license type="expression">MIT</license>
  
  <!-- The icon is used in Visual Studio's package manager UI -->
  <iconUrl>$iconUrl$</iconUrl>

  <!-- 
      If true, this value prompts the user to accept the license when
      installing the package. 
  -->
  <requireLicenseAcceptance>false</requireLicenseAcceptance>

  <!-- Any details about this particular release -->
  <releaseNotes>$releaseNotes$</releaseNotes>

  <!-- 
      The description can be used in package manager UI. Note that the
      nuget.org gallery uses information you add in the portal. 
  -->
  <description>$description$</description>

  <!-- Copyright information -->
  <copyright>$copyright$</copyright>

  <!-- Tags appear in the gallery and can be used for tag searches -->
  <tags>$tags$</tags>

  <!-- Dependencies are automatically installed when the package is installed -->
  <dependencies>
    <dependency id="Microsoft.AspNetCore.Authentication" version="2.2.0" />
    <dependency id="Microsoft.AspNetCore.Authentication.Cookies" version="2.2.0" />
    <dependency id="Microsoft.IdentityModel.Tokens" version="6.7.2-preview-10803222715" />
    <dependency id="System.IdentityModel.Tokens.Jwt" version="6.7.2-preview-10803222715" />
  </dependencies>
</metadata>

<!-- A package icon downloaded when the package is installed -->
<files>
  <file src="jwt-icon.png" target="jwt-icon.png" />
  <file src="README.md" target="README.md" />
</files>
</package>